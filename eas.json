{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "development"
      }
    },
    "preview": {
      "distribution": "store",
      "channel": "preview",
      "autoIncrement": true,
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "production"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleTeamId": "KUFRY2HT2D",
        "bundleIdentifier": "com.dannyhw.blogexample"
      },
      "android": {
        "applicationId": "com.dannyhw.blogexample"
      }
    },
    "preview": {
      "ios": {
        "appleTeamId": "KUFRY2HT2D",
        "bundleIdentifier": "com.dannyhw.blogexample-preview",
        "appName": "Blog Example preview"
      },
      "android": {
        "applicationId": "com.dannyhw.blogexample-preview"
      }
    }
  }
}
